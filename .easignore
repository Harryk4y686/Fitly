yarn-debug.log*
yarn-error.log*

# Build outputs
dist/
build/
.expo/
.expo-shared/

# Cache directories
.cache/
.parcel-cache/
.next/
.nuxt/

# IDE and editor files
.vscode/
.idea/
*.swp
*.swo
*~

# OS generated files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

